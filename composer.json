{
    "name": "osmscripts\/osm",
    "description": "Code generation and maintenance commands for Osm framework-based project.",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Vladislav Osmianskij",
            "email": "vlaosm@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "OsmScripts\\Osm\\": "src\/"
        }
    },
    "require": {
        "php": "^7.2",
        "osmscripts\/core": "dev-master@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "osm": {
            "commands": {
                "create:module": "OsmScripts\\Osm\\Commands\\CreateModule",
                "create:package": "OsmScripts\\Osm\\Commands\\CreatePackage",
                "modules": "OsmScripts\\Osm\\Commands\\Modules",
                "packages": "OsmScripts\\Osm\\Commands\\Packages",
                "create:command": "OsmScripts\\Osm\\Commands\\CreateCommand",
                "backup": "OsmScripts\\Osm\\Commands\\Backup",
                "restore": "OsmScripts\\Osm\\Commands\\Restore",
                "create:page": "OsmScripts\\Osm\\Commands\\CreatePage",
                "create:route": "OsmScripts\\Osm\\Commands\\CreateRoute",
                "translate": "OsmScripts\\Osm\\Commands\\Translate",
                "in": "OsmScripts\\Osm\\Commands\\In"
            }
        }
    },
    "bin": [
        "osm"
    ]
}